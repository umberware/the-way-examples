{
    "name": "the-way-examples",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build-main-only-example": "tsc --p tsconfig-main-only.json",
        "serve-main-only-example": "ts-node -P tsconfig-main-only.json examples/main-only/src/main/main.ts",
        "build-heroes-rest-example": "tsc --p tsconfig-heroes-rest.json",
        "serve-heroes-rest-example": "ts-node -P tsconfig-heroes-rest.json examples/heroes-rest/src/main/main.ts --the-way.core.scan.enabled=true --the-way.core.scan.path=dist/examples/heroes-rest/src/main",
        "run-heroes-rest-example": "yarn build-heroes-rest-example && node dist/examples/heroes-rest/src/main/main.js --the-way.core.scan.enabled=true --the-way.core.scan.path=dist/examples/heroes-rest/src/main",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/umberware/the-way-examples.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/umberware/the-way-examples/issues"
    },
    "homepage": "https://github.com/umberware/the-way-examples#readme",
    "devDependencies": {
        "@types/node": "^15.3.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "@umberware/the-way": "^1.0.0-Beta.1.5",
        "rxjs": "6.6.7"
    }
}
